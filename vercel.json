{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "ignore": [
    "gweasy.py",
    "*.md",
    "__pycache__/**",
    ".git/**",
    "node_modules/**",
    "tests/**",
    "docs/**",
    ".env",
    "*.pyc"
  ]
}
